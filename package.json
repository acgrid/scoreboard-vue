{
  "name": "scroeboard-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "cross-env NODE_ENV=development yarn build:server && npm-run-all --parallel watch:server watch:bundle",
    "build:server": "webpack --progress --colors --config server/webpack.conf.js",
    "watch:bundle": "cross-env NODE_ENV=development DEBUG=sc:* node-dev dist-server/bundle-dev.js",
    "watch:server": "cross-env NODE_ENV=development webpack --progress --colors --watch --config server/webpack.conf.js"
  },
