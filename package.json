    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "cross-env": "^6.0.0",
    "debug": "^4.1.1",
    "dot-prop": "^5.1.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-json-error-handler": "^3.0.0",
    "loadsh": "^0.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "register-service-worker": "^1.6.2",
